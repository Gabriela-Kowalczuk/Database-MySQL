CREATE TABLE [dbo].[hotele](
	[hotele_id] [int] IDENTITY(1,1) NOT NULL,
	[hotele_nazwa] [varchar](30) NOT NULL,
	[hotele_kraje_id] [int] NOT NULL,
	[hotele_miasta_id] [int] NOT NULL,
	[hotele_adres] [varchar](50) NOT NULL,
	[hotele_gwiazdki] [int] NOT NULL,
	[hotele_telefon] [varchar](20) NOT NULL,
	[hotele_email] [varchar](50) NOT NULL,
	[hotele_adres_www] [varchar](50) NOT NULL,
 CONSTRAINT [PK__hotele__45FE7E2677F1612F] PRIMARY KEY CLUSTERED 
(
	[hotele_id] ASC
)WITH (PAD_INDEX = OFF, STATISTICS_NORECOMPUTE = OFF, IGNORE_DUP_KEY = OFF, ALLOW_ROW_LOCKS = ON, ALLOW_PAGE_LOCKS = ON) ON [PRIMARY]
) ON [PRIMARY]
GO
/****** Object:  View [dbo].[liczba_gwiazdek]    Script Date: 22/12/2019 12:21:20 ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO

	CREATE VIEW [dbo].[liczba_gwiazdek]
AS 
	SELECT 
	 H.hotele_nazwa AS Nazwa
    ,H.hotele_adres AS Adres
    ,H.hotele_telefon AS Telefon
	,H.hotele_gwiazdki AS gwiazdki
	FROM Hotele H
	WHERE
    H.hotele_gwiazdki >= 3 AND H.hotele_gwiazdki <=5
GO
