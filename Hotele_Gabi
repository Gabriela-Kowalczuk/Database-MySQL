/****** Object:  View [dbo].[płeć]    Script Date: 22/12/2019 12:21:20 ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE VIEW [dbo].[płeć]
AS
SELECT klienci_imie, klienci_nazwisko, klienci_plec
FROM klienci
	WHERE 
	Klienci.klienci_plec = 'k' ;
    
GO
