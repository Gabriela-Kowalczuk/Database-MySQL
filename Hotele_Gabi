/****** Object:  View [dbo].[zlozone]    Script Date: 22/12/2019 12:21:20 ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE VIEW [dbo].[zlozone]
AS
SELECT TOP (6) K.kraje_nazwa_kraju AS Kraj, M.miasta_nazwa AS Miasto, H.hotele_id AS [Nazwa hotelu], COUNT(*) AS [Ilość złożonych rezerwacji]
FROM   dbo.rezerwacje AS RE INNER JOIN
             dbo.pokoje AS P ON RE.rezerwacje_pokoje_id = P.pokoje_id INNER JOIN
             dbo.hotele AS H ON P.pokoje_hotele_id = H.hotele_id INNER JOIN
             dbo.kraje AS K ON H.hotele_kraje_id = K.kraje_id INNER JOIN
             dbo.miasta AS M ON H.hotele_miasta_id = M.miasta_id
WHERE (RE.rezerwacje_czy_zrealizowano = 1)
GROUP BY K.kraje_nazwa_kraju, M.miasta_nazwa, H.hotele_id
ORDER BY [Ilość złożonych rezerwacji] DESC
GO
/****** Object:  View [dbo].[gwiazdki]    Script Date: 22/12/2019 12:21:20 ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE VIEW [dbo].[gwiazdki]
AS
 SELECT 
    CASE    
        WHEN hotele_gwiazdki = 1 THEN 1
        WHEN hotele_gwiazdki = 2 THEN 2
        WHEN hotele_gwiazdki = 3 THEN 3 
        WHEN hotele_gwiazdki = 4 THEN 4 
        WHEN hotele_gwiazdki = 5 THEN 5 
    END AS Liczba_gwiazdek
    ,COUNT(*) AS Liczba_hoteli

FROM
    hotele
        
GROUP by
    hotele_gwiazdki 
GO
