CREATE TABLE [dbo].[rezerwacje](
	[rezerwacje_id] [bigint] IDENTITY(1,1) NOT NULL,
	[rezerwacje_klienci_id] [int] NOT NULL,
	[rezerwacje_zameldowanie] [date] NOT NULL,
	[rezerwacje_wymeldowanie] [date] NOT NULL,
	[rezerwacje_pokoje_id] [int] NOT NULL,
	[rezerwacje_parking_id] [int] NULL,
	[rezerwacje_czy_zrealizowano] [bit] NOT NULL,
 CONSTRAINT [PK__rezerwac__A276CB15B2798B6A] PRIMARY KEY CLUSTERED 
(
	[rezerwacje_id] ASC
)WITH (PAD_INDEX = OFF, STATISTICS_NORECOMPUTE = OFF, IGNORE_DUP_KEY = OFF, ALLOW_ROW_LOCKS = ON, ALLOW_PAGE_LOCKS = ON) ON [PRIMARY]
) ON [PRIMARY]
